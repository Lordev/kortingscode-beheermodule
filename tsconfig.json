{
	"compilerOptions": {
		"target": "es5",
		"lib": ["dom", "dom.iterable", "esnext"],
		"allowJs": true,
		"skipLibCheck": true,
		"strict": true,
		"forceConsistentCasingInFileNames": true,
		"module": "esnext",
		"moduleResolution": "node",
		"resolveJsonModule": true,
		"isolatedModules": true,
		"jsx": "react-jsx",
		"baseUrl": "./src",
		"paths": {
			"*": ["*"]
		},
		"allowSyntheticDefaultImports": true,
		"esModuleInterop": true
	},
	"include": ["src"]
}
